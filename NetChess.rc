    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "O&ptions\tCtrl+P",            ID_EDIT_OPTIONS
        MENUITEM "Properties\tCtrl+R",          ID_EDIT_PROPERTIES
    END
    POPUP "&Tools"
    BEGIN
        MENUITEM "&Client\tCtrl+C",             ID_TOOLS_CLIENT
        MENUITEM "&Server\tCtrl+E",             ID_TOOLS_SERVER
        MENUITEM "&Disconnect\tCtrl+D",         ID_TOOLS_DISCONNECT
