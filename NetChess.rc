        MENUITEM "&Drop\tAlt+Ctrl+R",           ID_TOOLS_RESIGN
        MENUITEM "Sho&w\tAlt+Ctrl+S",           ID_TOOLS_SHOW
    END
    POPUP "&View"
    BEGIN
        MENUITEM "H&istory\tCtrl+I",            ID_VIEW_HISTORY
        MENUITEM "&Hide\tCtrl+H",               ID_VIEW_HIDE
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About NetRummy...",          ID_APP_ABOUT
        MENUITEM "&How to play",                ID_HELP_HOWTOPLAY
    END
END

